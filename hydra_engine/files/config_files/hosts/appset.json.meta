# .meta
# comment files

# File metadata

FILE:
  type: 'json'
  path: 'files/config_files/hosts/appset.json' # Optional/ default: path w/o .meta

PARAMS:
  - ConnectionStrings/Rex: # Input_URL
      type: string
      sub_type: # optional for array
      readonly: False
      description: "REx connection string"
      output_url: "root/constr/service"
      render:
        control: input_control
        display_name: "Rex conn url"
        constraints:
          - maxlength: 23
          - minlength: 21
        additional: # specified info
  - AllowedHosts: # Input_URL
      type: array
      sub_type: string
      readonly: False
      description: "Allowed hosts in infras"
      output_url: "root/constr/infras"
      render:
        control: input_control
        display_name: "allowed hosts"
        constraints:
        additional: # specified info
  - Auth/RequireHttpsMetadata: # Input_URL
      type: bool
      readonly: False
      sub_type: # optional for array
      description: "auth info"
      output_url: "root/constr"
      render:
        control: checkbox_control
        display_name: "auth require https Metadata"
        constraints:
  - key1:
      type: array
      readonly: False
      sub_type: datetime
      description: "auth info"
      output_url: "root/constr"
      render:
        control: datetime_control
        display_name: "Datetime array"
        constraints: