      default_value:
      type: dict
      sub_type:
      sub_type_schema:
        users_dict:
          default_value:
          type: array
          sub_type: composite
          sub_type_schema:
            username:
              type: string
              default_value: ""
              sub_type:
              sub_type_schema:
              readonly: [True]
              description: ""
              render:
                control: input_control
                display_name: "User name"
                constraints:
            ssh_key:
              type: string
              default_value: ""
              sub_type:
              sub_type_schema:
              readonly: [True]
              description: ""
              render:
                control: textarea_control
                display_name: "Ssh key"
                constraints:
