# File metadata

FILE:
  type: 'yaml'
  path: 'files/vars.yml' # Optional/ default: path w/o .meta

PARAMS:
  - image_name: # Input_URL
      type: string
      sub_type: # optional for array
      readonly: False
      description: "Docker image name"
      id: 6
      render:
        control: input_control
        display_name: "Docker image name"
        constraints:
          
  - container_name: # Input_URL
      type: string
      sub_type: # optional for array
      readonly: False
      description: "Name of the docker container to deploy"
      id: 6
      render:
        control: input_control
        display_name: "Docker container name"
        constraints:
          
  - image_tag: # Input_URL
      type: string
      sub_type: # optional for array
      readonly: False
      description: "Docker image tag"
      id: 6
      render:
        control: input_control
        display_name: "Tag of image"
        constraints:
          
  - env/test: # Input_URL
      type: string
      sub_type: # optional for array
      readonly: False
      description: "Map of Env variables to set for container"
      id: 6
      render:
        control: input_control
        display_name: "Env name"
        constraints:
  - ports: # Input_URL
      type: array
      sub_type: range
      readonly: False
      description: "Ports"
      id: 6
      render:
        control: range_control
        display_name: "Ports"
        constraints:
          