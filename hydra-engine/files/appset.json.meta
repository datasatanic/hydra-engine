# File metadata

FILE:
  type: 'json'
  path: 'appset.json' # Optional/ default: path w/o .meta

PARAMS:
  - ConnectionStrings/Rex: # Input_URL
      type: string
      sub_type: # optional for array
      readonly: True
      description: "REx connection string"
      output_url: "root/constr/service"
      render:
        ??? control: input_control
        display_name: "rex conn url"
        constraints:
          - cons1:
              args:
                -
          - cons2:
              args:
                -
                -
        additional: # specified info
  - ConnectionStrings/CONSUL_ADDR: # Input_URL
      type: string
      readonly: True
      sub_type: # optional for array
      description: "Consul connection string"
      output_url: "root/constr/service"
      render:
        ??? control: input_control
        display_name: "consul conn url"
        constraints:
          - cons1:
              args:
                -
          - cons2:
              args:
                -
                -
  - AllowedHosts: # Input_URL
      type: array
      sub_type: string
      readonly: False
      description: "Allowed hosts in infras"
      output_url: "root/constr/infras"
      render:
        ??? control: label_control
        display_name: "allowed hosts"
        constraints:
          - cons1:
              args:
                -
          - cons2:
              args:
                -
                -
        additional: # specified info

  - Auth/RequireHttpsMetadata: # Input_URL
      type: bool
